@model Estapar.Models.Manobra

@{
    ViewData["Title"] = "Detalhes da manobra";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detalhes da manobra</h1>

<div>
    <h4>Manobra</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataManobra)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataManobra)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Manobrista)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Manobrista.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Veiculo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Veiculo.Marca)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ManobraId">Alterar</a> |
    <a asp-action="Index">Voltar para lista</a>
</div>